{
  "material-icon-theme.files.associations": {
    ".env.docker-compose": "tune",
    ".env.docker-compose.example": "tune"
  },
  "files.associations": {
    ".env": "properties",
    ".env.*": "properties",
    "*.css": "tailwindcss"
  },
  "editor.tabSize": 4,
  "tailwindCSS.classAttributes": ["class", "className", ".*Styles"],
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["clsx\\(.*\\)", "(?:'|\"|`)([^,)]*)(?:'|\"|`)"],
    ["cntl`([^`]*)"]
  ]
}
